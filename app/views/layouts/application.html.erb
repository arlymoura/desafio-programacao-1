<!DOCTYPE html>
<html>

<head>
  <title>HexactaDev</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>

  <%= stylesheet_link_tag 'application', media: 'all' %>
  <%= javascript_pack_tag 'application' %>
</head>

<body>
  <%= render 'layouts/partials/navBar'%>
  <div class="container-fluid">
    <div class="row">
      <%= render 'layouts/partials/sideMenu'%>
      <main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-md-4 margin-main">
        <%= render 'layouts/partials/flash_message'%>
        <%= yield %>
      </main>
    </div>
  </div>
</body>

</html>